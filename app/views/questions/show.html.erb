<div ng-app="app" class="page-header">
  {{ 1 + 5 }}
  <h1>Survey Questions</h1>
  <div ng-controller="brainCtrl" ng-init="setup()" class="thumbnail">
    <div id="questions">{{ currentQuestion }}</div>
    <button ng-repeat="choice in choices" ng-click="submitAnswer(choice.id)">{{ choice.answer }}</button>
  </div>
</div>
